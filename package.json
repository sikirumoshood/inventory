{
  "name": "inventory-api",
  "version": "0.0.1",
  "description": "A custom inventory service",
  "keywords": [
    "inventory"
  ],
  "engines": {
    "node": "10.16.3"
  },
  "private": false,
  "main": "dist/index.js",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "migrate": "node_modules/db-migrate/bin/db-migrate up -e production",
    "test": "nyc mocha --require @babel/register './__tests__/**/**.spec.js'",
    "test:watch": "mocha --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "start:dev": "source .env && nodemon src/index.ts",
    "start": "npm run migrate && node dist/index.js",
    "build": "tsc -p ."
  },
  "author": {
    "name": "Sikiru moshood"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^0.5.1",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "pg-promise": "^10.10.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.37",
    "chai": "^4.3.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "sinon": "^5.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
